  <body>
  
  <nav class="pink accent-4">
    <div class="container">
      <ul class="left">
        <% if logged_in? %> 
          <li><a href="/dream_posts">Home</a></li>
          <li><a href="/dream_posts/new">New Dream Post</a></li>
          <li><a href="/logout">Log Out</a></li>
        <% end %>
      </ul>
    </div>
  </nav>
    <div class="wrapper">

        <%= yield %>

        <% if flash[:message] %>
          <h5 style="color:blue;"><%= flash[:message] %></h5>
        <% end %>
        <% if flash[:errors] %>
          <h5 style="color:red;"><%= flash[:message] %></h5>
        <% end %>

      <footer class="branding">
        <small>&copy; 2020 <strong>SinatraDreamSpaceProj</strong></small>
      </footer>
    </div>

    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1/jquery.min.js"></script>
